@model IEnumerable<MyProject.Models.FAQ>

@{
    ViewBag.Title = "FAQ";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="container-fluid">
    <h2>FAQ</h2>

    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table table-bordered display" id="table_id">
        <thead>
            <tr style="color:white">
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.f_qus)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.f_ans)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr style="color:white">
                    <td>
                        @Html.DisplayFor(modelItem => item.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.f_qus)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.f_ans)
                    </td>
                    <td>
                        @Html.ActionLink("EDIT", "FAQEDIT", new
                        {
                            id = item.f_id
                        }) |
                        @Html.ActionLink("Details", "Details", new
                        {
                            id = item.f_id
                        }) |
                        @Html.ActionLink("Delete", "Delete", new
                        {
                            id = item.f_id
                        })
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>